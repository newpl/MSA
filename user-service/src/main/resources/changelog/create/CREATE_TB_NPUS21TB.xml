<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
	<property name="now" value="now()" dbms="mysql" />
	<property name="now" value="now()" dbms="h2" />

	<!-- Role 테이블 스키마 정의 -->
    <changeSet id="CREATE_TB_NPUS21TB" author="newpl">
        <createTable tableName="NPUS21TB">
            <column name="ROLE_NO" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="USER_NO" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="DELETE_YN" type="varchar(1)" defaultValue="N">
    			<constraints nullable="false"/>
            </column>
            <column name="CREATED_DATETIME" type="timestamp" defaultValueDate="${now}">
    			<constraints nullable="false"/>
            </column>
            <column name="UPDATED_DATETIME" type="timestamp" defaultValueDate="${now}">
    			<constraints nullable="false"/>
            </column>
            <column name="CREATED_SOURCE_ID" type="varchar(120)"/>
            <column name="UPDATED_SOURCE_ID" type="varchar(120)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>